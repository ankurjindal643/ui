   <script>
      $(function() {
		
	$("#slideshow > div:gt(0)").hide();
        var slide;
        StartSlide();

        function InitSlide()
        {
               $('#slideshow > div:first')
		   .fadeOut(1000)
		   .next()
		   .fadeIn(1000)
		   .end()
		   .appendTo('#slideshow');
			        
                }
                             
        function StartSlide()
        {
        slide = setInterval(function(){InitSlide()},10000);
        }

        function StopSlide()
        {
        clearInterval(slide);
        }
        $('#slideshow').mouseover(function(){
        StopSlide();
        });
        $('#slideshow').mouseout(function(){
        StartSlide();
        });
       
       
});
   </script>
   
